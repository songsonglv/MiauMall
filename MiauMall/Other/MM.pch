NS_INLINE UIFont *kFONT (CGFloat font) {
    return [UIFont systemFontOfSize:font];
}

//适配字体大小
NS_INLINE CGFloat kFontSize(CGFloat size) {
    return size / 375.0 * WIDTH;
}

//按宽比例度适配
NS_INLINE CGFloat kReaValue(CGFloat with) {
    return with  / 375.0 * WIDTH;
}

